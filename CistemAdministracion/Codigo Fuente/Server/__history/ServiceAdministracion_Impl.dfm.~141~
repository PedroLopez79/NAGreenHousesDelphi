            DataType = datFloat
          end
          item
            Name = 'COSTO'
            DataType = datFloat
          end
          item
            Name = 'BODEGA'
            DataType = datInteger
          end>
      end
      item
        Params = <
          item
            Name = 'TurnoID'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'InvIni'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'Resurtidos'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'InvFin'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'Venta'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'Fecha'
            Value = '01/01/2010'
            ParamType = daptInput
          end
          item
            Name = 'ProductoID'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'EstacionID'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'AlmacenID'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'Existencia'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'Costo'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'Precio'
            Value = '1'
            ParamType = daptInput
          end>
        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'declare @TurnoID int'#10'declare @InvIni float'#10'declare @Resurtidos  ' +
              'float'#10'declare @InvFin varchar(70)'#10'declare @Venta int'#10'declare @Fe' +
              'cha DateTime'#10'declare @ProductoID int'#10'declare @EstacionID int'#10'dec' +
              'lare @AlmacenID int'#10'declare @Existencia int'#10'declare @Costo float' +
              #10'declare @Precio float'#10#10'declare @Resultado varchar(50)'#10#10'select @' +
              'TurnoID = (Select :TurnoID)'#10'select @InvIni = (Select :InvIni)'#10'se' +
              'lect @Resurtidos  = (Select :Resurtidos)'#10'select @InvFin = (Selec' +
              't :InvFin)'#10'select @Venta = (Select :Venta)'#10'select @Fecha = (Sele' +
              'ct :Fecha)'#10'select @ProductoID = (Select :ProductoID)'#10'select @Est' +
              'acionID = (Select :EstacionID)'#10'select @AlmacenID = (Select :Alma' +
              'cenID)'#10'select @Existencia = (Select :Existencia)'#10'select @Costo =' +
              ' (Select :Costo)'#10'select @Precio = (Select :Precio)'#10#10'  if ((selec' +
              't count(*) from dbo.LiquidacionProdIsla where Fecha = @Fecha AND' +
              ' ProductoID = @ProductoID'#10'       AND EstacionID = @EstacionID AN' +
              'D AlmacenID = @AlmacenID) > 0)'#10'  begin'#10#10'    if (select Categoria' +
              'ID from dbo.Producto where ProductoID = @ProductoID) = 2 or'#10'    ' +
              '   (select CategoriaID from dbo.Producto where ProductoID = @Pro' +
              'ductoID) = 3'#10'    begin'#10'    update dbo.LiquidacionProdIsla set'#10'  ' +
              '         TurnoID = @TurnoID,'#10'           InvIni  = @InvIni,'#10'     ' +
              '      Resurtidos = @Resurtidos,'#10'           Venta = @Venta,'#10'     ' +
              '      Fecha = @Fecha,'#10'           ProductoID = @ProductoID,'#10'     ' +
              '      EstacionID = @EstacionID,'#10'           AlmacenID = @AlmacenI' +
              'D,'#10'           Existencia = @Existencia,'#10'           Costo = @Cost' +
              'o,'#10'           Precio = @Precio'#10'    where Fecha = @Fecha AND Prod' +
              'uctoID = @ProductoID'#10'       AND EstacionID = @EstacionID AND Alm' +
              'acenID = @AlmacenID And Fecha= @Fecha and ProductoID = @Producto' +
              'ID'#10#10'    select @Resultado = '#39'Actualizacion'#39#10'    end'#10'  end'#10#10'  els' +
              'e'#10'  begin'#10#10'    if (select CategoriaID from dbo.Producto where Pr' +
              'oductoID = @ProductoID) = 2 or'#10'       (select CategoriaID from d' +
              'bo.Producto where ProductoID = @ProductoID) = 3'#10'    begin'#10'    IN' +
              'SERT'#10'    INTO dbo.LiquidacionProdIsla'#10'    (TurnoID, InvIni, Resu' +
              'rtidos, Venta, Fecha, ProductoID, EstacionID, AlmacenID, Existen' +
              'cia, Costo, Precio)'#10'    VALUES'#10'    (@TurnoID, @InvIni, @Resurtid' +
              'os, @Venta, @Fecha, @ProductoID, @EstacionID, @AlmacenID, @Exist' +
              'encia, @Costo, @Precio)'#10#10'    select @Resultado = '#39'Insercion'#39#10'   ' +
              ' end'#10'  end'#10#10'Select @Resultado as Resultado'#10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'Resultado'
                TableField = 'Resultado'
              end>
          end>
        Name = 'spLiquidacionProdIsla'
        Fields = <
          item
            Name = 'Resultado'
            DataType = datString
            Size = 50
          end>
      end
      item
        Params = <
          item
            Name = 'Fecha'
            Value = '01/01/2010'
            ParamType = daptInput
          end
          item
            Name = 'Estacion'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'TurnoID'
            Value = '1'
            ParamType = daptInput
          end>
        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'Declare @FechaIni as Datetime'#10'Declare @FechaFin as Datetime'#10'Decl' +
              'are @TurnoID as int'#10'Declare @Estacion as int'#10#10'Select @FechaIni =' +
              ' (Select :Fecha)'#10'Select @FechaFin = (Select :Fecha)'#10'Select @Esta' +
              'cion = (Select :Estacion)'#10'Select @TurnoID = (Select :TurnoID)'#10#10'D' +
              'ECLARE @ESTACIONID AS INT'#10'DECLARE @NOMBREESTACION AS VARCHAR (30' +
              ')'#10'DECLARE @PRODUCTOID AS INT'#10'DECLARE @CODIGOPRODUCTO AS VARCHAR(' +
              '30)'#10'DECLARE @PRODUCTONOMBRE AS VARCHAR(50)'#10'DECLARE @EXISTENCIA A' +
              'S INT'#10'DECLARE @VENTA AS INT'#10'DECLARE @RESUTIDOS AS INT'#10'DECLARE @I' +
              'NVINI AS INT'#10'DECLARE @PRECIOVENTA AS FLOAT'#10'DECLARE @COSTO AS FLO' +
              'AT'#10'DECLARE @NOMBREALMACEN AS VARCHAR (50)'#10'DECLARE @IDALMACEN AS ' +
              'INT'#10#10'CREATE TABLE #TEMP('#10'ESTACIONID INT,'#10'NOMBREESTACION VARCHAR ' +
              '(30),'#10'PRODUCTOID INT,'#10'CODIGOPRODUCTO VARCHAR(30),'#10'PRODUCTONOMBRE' +
              ' VARCHAR(50),'#10'EXISTENCIA INT,'#10'VENTA INT,'#10'RESUTIDOS INT,'#10'INVINI I' +
              'NT,'#10'PRECIOVENTA FLOAT,'#10'COSTO FLOAT,'#10'NOMBREALMACEN VARCHAR(50),'#10'I' +
              'DALMACEN INT'#10')'#10#10'DECLARE prueba_Cursor SCROLL CURSOR FOR'#10'SELECT E' +
              'stacion.EstacionID, Estacion.Nombre, Producto.ProductoID, Produc' +
              'to.Codigo, Producto.Nombre,'#10'       SUM(DetalleMovimientoAlmacen2' +
              '.Cantidad * TipoMovimientoAlmacen2.Operador) AS Existencia, 0,'#10' ' +
              '      0, 0, Producto.PrecioVenta, Producto.Costo, Almacen2.Almac' +
              'enID, Almacen2.Nombre'#10'             FROM          Almacen2 INNER ' +
              'JOIN'#10'             MovimientoAlmacen2 ON Almacen2.AlmacenID = Mov' +
              'imientoAlmacen2.AlmacenID INNER JOIN'#10'             DetalleMovimie' +
              'ntoAlmacen2 ON'#10'             MovimientoAlmacen2.MovimientoAlmacen' +
              'ID = DetalleMovimientoAlmacen2.MovimientoAlmacenID INNER JOIN'#10'  ' +
              '           TipoMovimientoAlmacen2 ON'#10'             MovimientoAlma' +
              'cen2.TipoMovimientoAlmacenID = TipoMovimientoAlmacen2.TipoMovimi' +
              'entoAlmacenID INNER JOIN'#10'             Producto ON DetalleMovimie' +
              'ntoAlmacen2.ProductoID = Producto.ProductoID INNER JOIN'#10'        ' +
              '     Estacion ON MovimientoAlmacen2.EstacionID = Estacion.Estaci' +
              'onID'#10'WHERE      (MovimientoAlmacen2.EstacionID = @Estacion AND M' +
              'ovimientoAlmacen2.Fecha <= @FechaFin) and Producto.ProductoID > ' +
              '3'#10'            AND ALMACEN2.ESTACIONID = @ESTACION'#10'            GR' +
              'OUP BY Producto.Nombre, Producto.ProductoID, Producto.Codigo, Es' +
              'tacion.EstacionID, Estacion.Nombre,'#10'            Producto.PrecioV' +
              'enta, Producto.Costo, Almacen2.AlmacenID, Almacen2.Nombre'#10'      ' +
              '      ORDER BY Almacen2.AlmacenID, Producto.Nombre'#10'OPEN prueba_C' +
              'ursor'#10'FETCH NEXT FROM prueba_Cursor'#10'into @ESTACIONID, @NOMBREEST' +
              'ACION, @PRODUCTOID, @CODIGOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCI' +
              'A,'#10'     @VENTA, @RESUTIDOS, @INVINI, @PRECIOVENTA, @COSTO, @IDAL' +
              'MACEN, @NOMBREALMACEN'#10'WHILE @@FETCH_STATUS = 0'#10'BEGIN'#10'  INSERT IN' +
              'TO #TEMP (ESTACIONID,NOMBREESTACION,PRODUCTOID,CODIGOPRODUCTO,PR' +
              'ODUCTONOMBRE,EXISTENCIA,'#10'                     VENTA,RESUTIDOS,IN' +
              'VINI,PRECIOVENTA,COSTO, NOMBREALMACEN, IDALMACEN)'#10'  VALUES(@ESTA' +
              'CIONID, @NOMBREESTACION, @PRODUCTOID, @CODIGOPRODUCTO, @PRODUCTO' +
              'NOMBRE, @EXISTENCIA,'#10'         @VENTA, @RESUTIDOS, @INVINI, @PREC' +
              'IOVENTA, @COSTO, @NOMBREALMACEN, @IDALMACEN)'#10#10'  FETCH NEXT FROM ' +
              'prueba_Cursor into @ESTACIONID, @NOMBREESTACION, @PRODUCTOID, @C' +
              'ODIGOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCIA,'#10'  @VENTA, @RESUTIDO' +
              'S, @INVINI, @PRECIOVENTA, @COSTO, @IDALMACEN, @NOMBREALMACEN'#10'END' +
              #10#10'CLOSE prueba_Cursor'#10'DEALLOCATE prueba_Cursor'#10#10#10'DECLARE prueba_' +
              'Cursor SCROLL CURSOR FOR'#10'SELECT Estacion.EstacionID, Estacion.No' +
              'mbre, Producto.ProductoID, Producto.Codigo, Producto.Nombre,'#10'   ' +
              '    0, SUM(DetalleLiquidacion2.Cantidad) AS Venta, 0, 0, Product' +
              'o.PrecioVenta,'#10'       IsNull(DetalleLiquidacion2.Costo,Producto.' +
              'Costo) as Costo, Almacen2.AlmacenID, Almacen2.Nombre'#10'       FROM' +
              '      DetalleLiquidacion2 INNER JOIN'#10'       DespachadorLiquidaci' +
              'on2 ON DetalleLiquidacion2.DespachadorLiquidacionID = Despachado' +
              'rLiquidacion2.DespachadorLiquidacionID INNER JOIN'#10'       Liquida' +
              'cion2 ON'#10'       DespachadorLiquidacion2.LiquidacionID = Liquidac' +
              'ion2.LiquidacionID INNER JOIN'#10'       Agrupacion2 ON DespachadorL' +
              'iquidacion2.AgrupacionID = Agrupacion2.AgrupacionID INNER JOIN'#10' ' +
              '      Almacen2 ON Almacen2.AlmacenID = Agrupacion2.AlmacenID INN' +
              'ER JOIN'#10'       Producto ON DetalleLiquidacion2.ProductoID = Prod' +
              'ucto.ProductoID INNER JOIN'#10'       Estacion ON Liquidacion2.Estac' +
              'ionID = Estacion.EstacionID'#10'       WHERE  (Liquidacion2.Estacion' +
              'ID = @Estacion)AND ALMACEN2.ESTACIONID = @ESTACION'#10'       AND (L' +
              'iquidacion2.Fecha Between @FechaIni AND @FechaFin)  and Producto' +
              '.ProductoID > 3'#10'       GROUP BY Estacion.EstacionID, Estacion.No' +
              'mbre, Producto.ProductoID, Producto.Codigo, Producto.Nombre,'#10'   ' +
              '    Producto.PrecioVenta, DetalleLiquidacion2.Costo,Producto.Cos' +
              'to, Almacen2.AlmacenID, Almacen2.Nombre'#10'       ORDER BY Almacen2' +
              '.AlmacenID, Producto.Nombre'#10'OPEN prueba_Cursor'#10'FETCH NEXT FROM p' +
              'rueba_Cursor'#10'into @ESTACIONID, @NOMBREESTACION, @PRODUCTOID, @CO' +
              'DIGOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCIA,'#10'     @VENTA, @RESUTI' +
              'DOS, @INVINI, @PRECIOVENTA, @COSTO, @IDALMACEN, @NOMBREALMACEN'#10'W' +
              'HILE @@FETCH_STATUS = 0'#10'BEGIN'#10'  INSERT INTO #TEMP (ESTACIONID,NO' +
              'MBREESTACION,PRODUCTOID,CODIGOPRODUCTO,PRODUCTONOMBRE,EXISTENCIA' +
              ','#10'                     VENTA,RESUTIDOS,INVINI,PRECIOVENTA,COSTO,' +
              ' NOMBREALMACEN, IDALMACEN)'#10'  VALUES(@ESTACIONID, @NOMBREESTACION' +
              ', @PRODUCTOID, @CODIGOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCIA,'#10'  ' +
              '       @VENTA, @RESUTIDOS, @INVINI, @PRECIOVENTA, @COSTO, @NOMBR' +
              'EALMACEN, @IDALMACEN)'#10#10'  FETCH NEXT FROM prueba_Cursor into @EST' +
              'ACIONID, @NOMBREESTACION, @PRODUCTOID, @CODIGOPRODUCTO, @PRODUCT' +
              'ONOMBRE, @EXISTENCIA,'#10'  @VENTA, @RESUTIDOS, @INVINI, @PRECIOVENT' +
              'A, @COSTO, @IDALMACEN, @NOMBREALMACEN'#10'END'#10#10'CLOSE prueba_Cursor'#10'D' +
              'EALLOCATE prueba_Cursor'#10#10'DECLARE prueba_Cursor SCROLL CURSOR FOR' +
              #10'Select Estacion.EstacionID, Estacion.Nombre, Producto.ProductoI' +
              'D, Producto.Codigo, Producto.Nombre,'#10'       0, 0, SUM(DetalleMov' +
              'imientoAlmacen2.Cantidad) as Resurtidos, 0, Producto.PrecioVenta' +
              ','#10'       IsNull(Producto.Costo, 0) as Costo, Almacen2.AlmacenID,' +
              ' Almacen2.Nombre'#10'       FROM'#10'       Almacen2 INNER JOIN'#10'       M' +
              'ovimientoAlmacen2 ON Almacen2.AlmacenID = MovimientoAlmacen2.Alm' +
              'acenID INNER JOIN'#10'       DetalleMovimientoAlmacen2 ON'#10'       Mov' +
              'imientoAlmacen2.MovimientoAlmacenID = DetalleMovimientoAlmacen2.' +
              'MovimientoAlmacenID INNER JOIN'#10'       TipoMovimientoAlmacen2 ON'#10 +
              '       MovimientoAlmacen2.TipoMovimientoAlmacenID = TipoMovimien' +
              'toAlmacen2.TipoMovimientoAlmacenID INNER JOIN'#10'       Producto ON' +
              ' DetalleMovimientoAlmacen2.ProductoID = Producto.ProductoID INNE' +
              'R JOIN'#10'       Estacion ON MovimientoAlmacen2.EstacionID = Estaci' +
              'on.EstacionID'#10'       WHERE (MovimientoAlmacen2.EstacionID = @Est' +
              'acion AND MovimientoAlmacen2.Fecha Between @FechaIni AND @FechaF' +
              'in'#10'       AND TipoMovimientoAlmacen2.Operador > -1)AND ALMACEN2.' +
              'ESTACIONID = @ESTACION'#10'       GROUP BY Estacion.EstacionID, Esta' +
              'cion.Nombre, Producto.ProductoID, Producto.Codigo, Producto.Nomb' +
              're,'#10'       Producto.PrecioVenta, Producto.Costo, Almacen2.Almace' +
              'nID, Almacen2.Nombre'#10'       ORDER BY Almacen2.AlmacenID, Product' +
              'o.Nombre'#10'OPEN prueba_Cursor'#10'FETCH NEXT FROM prueba_Cursor'#10'into @' +
              'ESTACIONID, @NOMBREESTACION, @PRODUCTOID, @CODIGOPRODUCTO, @PROD' +
              'UCTONOMBRE, @EXISTENCIA,'#10'     @VENTA, @RESUTIDOS, @INVINI, @PREC' +
              'IOVENTA, @COSTO, @IDALMACEN, @NOMBREALMACEN'#10'WHILE @@FETCH_STATUS' +
              ' = 0'#10'BEGIN'#10'  INSERT INTO #TEMP (ESTACIONID,NOMBREESTACION,PRODUC' +
              'TOID,CODIGOPRODUCTO,PRODUCTONOMBRE,EXISTENCIA,'#10'                 ' +
              '    VENTA,RESUTIDOS,INVINI,PRECIOVENTA,COSTO, NOMBREALMACEN, IDA' +
              'LMACEN)'#10'  VALUES(@ESTACIONID, @NOMBREESTACION, @PRODUCTOID, @COD' +
              'IGOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCIA,'#10'         @VENTA, @RES' +
              'UTIDOS, @INVINI, @PRECIOVENTA, @COSTO, @NOMBREALMACEN, @IDALMACE' +
              'N)'#10#10'  FETCH NEXT FROM prueba_Cursor into @ESTACIONID, @NOMBREEST' +
              'ACION, @PRODUCTOID, @CODIGOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCI' +
              'A,'#10'  @VENTA, @RESUTIDOS, @INVINI, @PRECIOVENTA, @COSTO, @IDALMAC' +
              'EN, @NOMBREALMACEN'#10'END'#10#10'CLOSE prueba_Cursor'#10'DEALLOCATE prueba_Cu' +
              'rsor'#10#10'DECLARE prueba_Cursor SCROLL CURSOR FOR'#10'Select Estacion.Es' +
              'tacionID, Estacion.Nombre, Producto.ProductoID, Producto.Codigo,' +
              ' Producto.Nombre,'#10'       0, 0, 0, SUM(DetalleMovimientoAlmacen2.' +
              'Cantidad * TipoMovimientoAlmacen2.Operador) as InvIni, Producto.' +
              'PrecioVenta,'#10'       IsNull(Producto.Costo,0) as Costo, Almacen2.' +
              'AlmacenID, Almacen2.Nombre'#10'       FROM'#10'       Almacen2 INNER JOI' +
              'N'#10'       MovimientoAlmacen2 ON Almacen2.AlmacenID = MovimientoAl' +
              'macen2.AlmacenID INNER JOIN'#10'       DetalleMovimientoAlmacen2 ON'#10 +
              '       MovimientoAlmacen2.MovimientoAlmacenID = DetalleMovimient' +
              'oAlmacen2.MovimientoAlmacenID INNER JOIN'#10'       TipoMovimientoAl' +
              'macen2 ON'#10'       MovimientoAlmacen2.TipoMovimientoAlmacenID = Ti' +
              'poMovimientoAlmacen2.TipoMovimientoAlmacenID INNER JOIN'#10'       P' +
              'roducto ON DetalleMovimientoAlmacen2.ProductoID = Producto.Produ' +
              'ctoID INNER JOIN'#10'       Estacion ON MovimientoAlmacen2.EstacionI' +
              'D = Estacion.EstacionID'#10'       WHERE (MovimientoAlmacen2.Estacio' +
              'nID = @Estacion AND MovimientoAlmacen2.Fecha <= @FechaFin)'#10'     ' +
              '  AND (ALMACEN2.ESTACIONID = @ESTACION)'#10'       GROUP BY Estacion' +
              '.EstacionID, Estacion.Nombre, Producto.ProductoID, Producto.Codi' +
              'go, Producto.Nombre,'#10'       Producto.PrecioVenta, Producto.Costo' +
              ', Almacen2.AlmacenID, Almacen2.Nombre'#10'       ORDER BY Almacen2.A' +
              'lmacenID, Producto.Nombre'#10'OPEN prueba_Cursor'#10'FETCH NEXT FROM pru' +
              'eba_Cursor'#10'into @ESTACIONID, @NOMBREESTACION, @PRODUCTOID, @CODI' +
              'GOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCIA,'#10'     @VENTA, @RESUTIDO' +
              'S, @INVINI, @PRECIOVENTA, @COSTO, @IDALMACEN, @NOMBREALMACEN'#10'WHI' +
              'LE @@FETCH_STATUS = 0'#10'BEGIN'#10'  INSERT INTO #TEMP (ESTACIONID,NOMB' +
              'REESTACION,PRODUCTOID,CODIGOPRODUCTO,PRODUCTONOMBRE,EXISTENCIA,'#10 +
              '                     VENTA,RESUTIDOS,INVINI,PRECIOVENTA,COSTO, N' +
              'OMBREALMACEN, IDALMACEN)'#10'  VALUES(@ESTACIONID, @NOMBREESTACION, ' +
              '@PRODUCTOID, @CODIGOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCIA,'#10'    ' +
              '     @VENTA, @RESUTIDOS, @INVINI, @PRECIOVENTA, @COSTO, @NOMBREA' +
              'LMACEN, @IDALMACEN)'#10#10'  FETCH NEXT FROM prueba_Cursor into @ESTAC' +
              'IONID, @NOMBREESTACION, @PRODUCTOID, @CODIGOPRODUCTO, @PRODUCTON' +
              'OMBRE, @EXISTENCIA,'#10'  @VENTA, @RESUTIDOS, @INVINI, @PRECIOVENTA,' +
              ' @COSTO, @IDALMACEN, @NOMBREALMACEN'#10'END'#10#10#10'CLOSE prueba_Cursor'#10'DE' +
              'ALLOCATE prueba_Cursor'#10#10'SELECT SUM(VENTA) AS VENTA, ESTACIONID,N' +
              'OMBREESTACION,PRODUCTOID,CODIGOPRODUCTO,PRODUCTONOMBRE, SUM(EXIS' +
              'TENCIA) AS EXISTENCIA,'#10'       SUM(RESUTIDOS) as RESURTIDOS, SUM(' +
              'INVINI)AS INVINI,PRECIOVENTA,COSTO, NOMBREALMACEN, IDALMACEN FRO' +
              'M #TEMP'#10'WHERE ESTACIONID = @Estacion'#10'GROUP BY ESTACIONID,NOMBREE' +
              'STACION,PRODUCTOID,CODIGOPRODUCTO,PRODUCTONOMBRE, PRECIOVENTA, C' +
              'OSTO, NOMBREALMACEN,'#10'         IDALMACEN'#10'ORDER BY NOMBREALMACEN, ' +
              'IDALMACEN, PRODUCTOID'#10#10'DROP TABLE #TEMP'#10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'VENTA'
                TableField = 'VENTA'
              end
              item
                DatasetField = 'ESTACIONID'
                TableField = 'ESTACIONID'
              end
              item
                DatasetField = 'NOMBREESTACION'
                TableField = 'NOMBREESTACION'
              end
              item
                DatasetField = 'PRODUCTOID'
                TableField = 'PRODUCTOID'
              end
              item
                DatasetField = 'CODIGOPRODUCTO'
                TableField = 'CODIGOPRODUCTO'
              end
              item
                DatasetField = 'PRODUCTONOMBRE'
                TableField = 'PRODUCTONOMBRE'
