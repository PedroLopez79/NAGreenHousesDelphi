              'ionEstacionID'#10'       WHERE PuntosPremio.AgrupacionEstacionID = A' +
              'grupacionEstacion.AgrupacionEstacionID AND Puntos <= :Puntos AND' +
              ' Puntos<>0) A'#10
            StatementType = stSQL
