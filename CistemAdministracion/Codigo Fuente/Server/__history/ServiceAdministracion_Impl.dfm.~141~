        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'Declare @FolioFacturaID Integer'#10'declare @resultado varchar(100)'#10 +
              'declare @fecha datetime'#10'declare @folio int'#10'declare @serie varcha' +
              'r(10)'#10'declare @usuarioid int'#10'declare @ID int'#10#10'select @Serie = (s' +
              'elect :Serie)'#10'select @Folio = (select :Folio)'#10'select @Fecha = (s' +
              'elect :Fecha)'#10'select @usuarioid = (select :UsuarioID)'#10'Select @Fo' +
              'lioFacturaID=(Select FacturaID from Factura where Folio=@Folio a' +
              'nd Serie=@Serie)'#10#10'if (@FolioFacturaID > 0)'#10'begin'#10'        if ((se' +
              'lect cancelada from factura where FacturaID=@FolioFacturaID) = 0' +
              ')'#10'        begin'#10'                Update Factura Set Cancelada=1, ' +
              'Subtotal=0, Impuesto=0, Total=0, usuarioid = @usuarioid'#10'        ' +
              '        Where FacturaID=@FolioFacturaID'#10#10'                Delete ' +
              'ticketFactura'#10'                where FacturaID=@FolioFacturaID'#10'  ' +
              '              '#10'                select @resultado = (select '#39'La f' +
              'actura que ingreso se cancelo correctamente'#39')'#10'        end'#10'      ' +
              '  else'#10'        begin'#10'                select @resultado = (select' +
              ' '#39'El folio de la factura que ingreso ya fue cancelado'#39')'#10'        ' +
              'end'#10'end'#10'else'#10'begin'#10'        select @id = (select Folio from folio' +
              #10'        where campo = '#39'FacturaID'#39' and serie = '#39#39')'#10#10'        upda' +
              'te folio set folio = folio + 1'#10'        where campo = '#39'FacturaID'#39 +
              ' and serie = '#39#39#10#10'        insert into Factura(FacturaID, Serie, F' +
              'olio, ejercicio, periodo, dia, fecha, subtotal, impuesto, total,' +
              ' impuestoporcentaje, cancelada, turno, estacionid, clienteid, fo' +
              'rmapagoid, tipofacturaid, usuarioid)'#10'        values (@id, @Serie' +
              ', @Folio, year(@fecha), month(@fecha), day(@fecha), @fecha, 0, 0' +
              ', 0, 0, 1, 1, :EstacionID, 0, 1, 1, @UsuarioID)'#10'        '#10'       ' +
              ' select @resultado = (select '#39'El folio de la factura que ingreso' +
              ' se creo y fue cancelado'#39')'#10'end'#10#10'if (@FolioFacturaID > 0)'#10'begin'#10' ' +
              ' Update FacturaElectronica Set Vigencia = 0'#10'  Where FacturaID=@F' +
              'olioFacturaID'#10'end'#10#10'select @Resultado as Resultado'#10
            StatementType = stSQL
