      item
        Params = <>
        Statements = <
          item
            Connection = 'Administracion'
            StatementType = stSQL
            ColumnMappings = <>
          end>
        Name = 'ActualizaCorteLiquidacion'
        Fields = <>
      end
      item
        Params = <
          item
            Name = 'EstacionID'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'TicketID'
            Value = '1'
            ParamType = daptInput
          end>
        Statements = <
          item
            Connection = 'Administracion'
            SQL = 
              'declare @Cuantos int'#10'declare @TicketID int'#10'declare @EstacionID i' +
              'nt'#10'declare @ClienteLiqID int'#10'declare @ClienteFacID int'#10#10'select @' +
              'TicketID = (select :TicketID)'#10'select @EstacionID = (Select :Esta' +
              'cionID)'#10#10'select @ClienteLiqID = ISNULL((SELECT     top 1 Detalle' +
              'Liquidacion2.ClienteID'#10'FROM         Liquidacion2 INNER JOIN'#10'    ' +
              '                  DespachadorLiquidacion2 ON Liquidacion2.Liquid' +
              'acionID = DespachadorLiquidacion2.LiquidacionID INNER JOIN'#10'     ' +
              '                 DetalleLiquidacion2 ON DespachadorLiquidacion2.' +
              'DespachadorLiquidacionID = DetalleLiquidacion2.DespachadorLiquid' +
              'acionID'#10'WHERE     (Liquidacion2.EstacionID = @EstacionID) AND (D' +
              'etalleLiquidacion2.Ticket = @TicketID)), 0)'#10#10'select @ClienteFacI' +
              'D = ISNULL((SELECT  top 1    Factura.ClienteID'#10'FROM         Fact' +
              'ura INNER JOIN'#10'                      TicketFactura ON Factura.Fa' +
              'cturaID = TicketFactura.FacturaID'#10'WHERE     (TicketFactura.Estac' +
              'ionID = @EstacionID) AND (TicketFactura.TicketID = @TicketID)), ' +
              '0)'#10#10'select @Cuantos = (select 0)  --NO EN FACTURA NO EN LIQUIDAC' +
              'ION'#10#10'if (@ClienteFacID > 0)'#10'begin'#10#9'select @Cuantos = (select 1) ' +
              '-- YA EN FACTURA'#10'end'#10#10'if (@ClienteLiqID > 0)'#10'begin'#10#9'select @Cuan' +
              'tos = (select 2)  --YA EN LIQUIDACION'#10'end'#10#10'if (((Select Facturay' +
              'Liquidacion from cliente where clienteid = @ClienteLiqID) = 1) a' +
              'nd (@ClienteFacID = 0) and (@ClienteLiqID > 0))'#10'begin'#10'     selec' +
              't @Cuantos = (select 3)  --EN LIQUIDACION NO EN FACTURA'#10'end'#10#10'if ' +
              '(((Select FacturayLiquidacion from cliente where clienteid = @Cl' +
              'ienteLiqID) = 1) and (@ClienteLiqID = 0) and (@ClienteFacID > 0)' +
              ')'#10'begin'#10'     select @Cuantos = (select 4) -- EN FACTURA NO EN LI' +
              'QUIDACION'#10'end'#10#10'select @Cuantos as Cuantos'#10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'Cuantos'
                TableField = 'Cuantos'
              end>
          end>
