        Statements = <
          item
            Connection = 'Administracion'
            SQL = 
              'declare @EstacionID as integer'#10'declare @AgrupacionID as integer'#10 +
              'declare @Tipo as varchar(2)'#10'declare @Secuencia as integer'#10'declar' +
              'e @Almacen as integer'#10#10'select @EstacionID = (select :EstacionID)' +
              #10'select @AgrupacionID = (select :AgrupacionID)'#10'select @Tipo = (s' +
              'elect :Tipo)'#10'select @Secuencia = (select :Secuencia)'#10'select @Alm' +
              'acen = (Select AlmacenID From Agrupacion Where AgrupacionID = @A' +
              'grupacionID)'#10#10#10'select p.ProductoId, p.Nombre, PrecioVenta,'#10#10'(cas' +
              'e'#10'  when'#10#9'(select Inventariofinal from LiquidacionProducto LP'#10#9'w' +
              'here LP.LiquidacionID = (select max(LiquidacionID)'#10#9'From Liquida' +
              'cion L Where AgrupacionID = @AgrupacionID and EstacionID = @Esta' +
              'cionID )'#10#9'and LP.ProductoID = p.ProductoID)  is null then 0'#10'  el' +
              'se'#10'       (select Inventariofinal from LiquidacionProducto LP'#10#9'w' +
              'here LP.LiquidacionID = (select max(LiquidacionID)'#10#9'From Liquida' +
              'cion L Where AgrupacionID = @AgrupacionID and EstacionID = @Esta' +
              'cionID )'#10#9'and LP.ProductoID = p.ProductoID)'#10'end) as Inventariofi' +
              'nal,'#10#10#10'(case'#10'when'#10'   (select sum(cantidad) from transaccion t in' +
              'ner join detallemovimiento d on'#10'   t.transaccionid = d.transacci' +
              'onid'#10'   where EstacionID=@EstacionID and Secuencia = @Secuencia ' +
              'and d.Tipo = @Tipo and d.ProductoId =p.ProductoID and d.operador' +
              ' =1 and AlmacenDestinoID = @Almacen) is null then 0'#10'else'#10'   (sel' +
              'ect sum(cantidad) from transaccion t inner join detallemovimient' +
              'o d on'#10'   t.transaccionid = d.transaccionid'#10'   where EstacionID=' +
              '@EstacionID and Secuencia = @Secuencia and d.Tipo = @Tipo and d.' +
              'ProductoId =p.ProductoID and d.operador =1 and AlmacenDestinoID ' +
              '= @Almacen)'#10'end'#10') as TraspasosRecibidos,'#10#10#10'(case'#10'when'#10'   (select' +
              ' sum(cantidad) from transaccion t inner join detallemovimiento d' +
              ' on'#10'   t.transaccionid = d.transaccionid'#10'   where EstacionID=@Es' +
              'tacionID and Secuencia = @Secuencia and d.Tipo = @Tipo and d.Pro' +
              'ductoId =p.ProductoID and d.operador = -1 and AlmacenID = @Almac' +
              'en) is null then 0'#10'else'#10'   (select sum(cantidad) from transaccio' +
              'n t inner join detallemovimiento d on'#10'   t.transaccionid = d.tra' +
              'nsaccionid'#10'   where EstacionID=@EstacionID and Secuencia = @Secu' +
              'encia and d.Tipo = @Tipo and d.ProductoId =p.ProductoID and d.op' +
              'erador = -1 and AlmacenID = @Almacen)'#10'end'#10') as TraspasosEnviados' +
              #10#10#10'From producto p'#10'Where (clavepemex is null or ClavePemex='#39#39' ) ' +
              'and Inventariable = 1'#10#10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'ProductoId'
                TableField = 'ProductoId'
              end
              item
                DatasetField = 'Nombre'
                TableField = 'Nombre'
