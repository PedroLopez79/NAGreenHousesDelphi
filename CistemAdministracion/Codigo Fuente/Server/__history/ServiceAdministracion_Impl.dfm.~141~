        Statements = <
          item
            Connection = 'Administracion'
            SQL = 
              #10'declare @EstacionID as integer'#10'declare @AlmacenID  as integer'#10'd' +
              'eclare @ProductoID as integer'#10'declare @Fecha      as DateTime'#10#10'S' +
              'elect @EstacionID = (Select :EstacionID)'#10'Select @AlmacenID  = (S' +
              'elect :AlmacenID)'#10'Select @ProductoID = (Select :ProductoID)'#10'Sele' +
              'ct @Fecha      = (Select :Fecha)'#10#10'Select sum(cantidad*operador) ' +
              'as Existencia'#10'  From (Transaccion T Inner Join DetalleMovimiento' +
              ' DM on T.TransaccionID = DM.TransaccionID)'#10'    inner join Movimi' +
              'entoAlmacen MA on DM.MovimientoAlmacen = MA.MovimientoAlmacen'#10'  ' +
              '    Where MA.EstacionID = @EstacionID and MA.AlmacenID = @Almace' +
              'nID and DM.ProductoID = @ProductoID'#10#9'and T.Fecha<=@Fecha'#10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'Existencia'
                TableField = 'Existencia'
              end>
