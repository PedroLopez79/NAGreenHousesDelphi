      item
        Params = <
          item
            Name = 'FacturaID'
            Value = '1'
            ParamType = daptInput
          end>
        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'Declare @FacturaID as Integer'#10'Select @FacturaID=(Select :Factura' +
              'ID)'#10'SELECT'#10'f.FacturaID, f.Folio, f.Serie, f.Ejercicio, f.Periodo' +
              ', f.Dia, f.Fecha,'#10'f.Subtotal, f.Impuesto, f.Total, f.ImpuestoPor' +
              'centaje, f.Turno, c.Localidad as LocCli,'#10'c.Municipio as MunCli, ' +
              'c.Estado as EstCli,'#10'f.Cancelada, f.EstacionID, f.ClienteID, f.Li' +
              'quidacionID, f.FormaPagoID,'#10'f.TipoFacturaID, f.Tickets, c.*, g.N' +
              'ombre as NombreEstacion, g.Domicilio as DomicilioEstacion,'#10'g.RFC' +
              ' as RFCEstacion, g.NoExterior as NoExteriorEst, g.Localidad as L' +
              'ocalidadEstacion,'#10'g.Municipio as MunicipioEstacion, g.Estado as ' +
              'EstadoEstacion, g.Pais as PaisEstacion,'#10'g.Colonia as ColoniaEsta' +
              'cion, g.CodigoPostal as CodigoPostalEstacion, e.RazonSocial as R' +
              'azonSocialEst,'#10'e.ClavePemex as ClavePemexEstacion, fe.CadenaOrig' +
              'inal, fe.SelloDigital, fp.Descripcion'#10'FROM'#10'dbo.Factura f'#10'inner j' +
              'oin dbo.Cliente c on c.ClienteID = f.ClienteID'#10'inner join dbo.Ga' +
              'solinero g on g.EstacionID = f.EstacionID'#10'inner join dbo.Estacio' +
              'n e on e.EstacionID = f.EstacionID'#10'inner join dbo.FacturaElectro' +
              'nica fe on fe.FacturaID = f.FacturaID'#10'inner join dbo.FormaPago f' +
              'p on fp.FormaPagoID = f.FormaPagoID'#10'WHERE f.FacturaID=@FacturaID' +
              #10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'FacturaID'
                TableField = 'FacturaID'
              end
              item
                DatasetField = 'Folio'
                TableField = 'Folio'
              end
              item
                DatasetField = 'Serie'
                TableField = 'Serie'
              end
              item
                DatasetField = 'Ejercicio'
                TableField = 'Ejercicio'
              end
              item
                DatasetField = 'Periodo'
                TableField = 'Periodo'
              end
              item
                DatasetField = 'Dia'
                TableField = 'Dia'
              end
              item
                DatasetField = 'Fecha'
                TableField = 'Fecha'
              end
