            Name = 'Costo'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'Precio'
            Value = '1'
            ParamType = daptInput
          end>
        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'declare @TurnoID int'#10'declare @InvIni float'#10'declare @Resurtidos  ' +
              'float'#10'declare @InvFin varchar(70)'#10'declare @Venta int'#10'declare @Bo' +
              'dega int'#10'declare @Fecha DateTime'#10'declare @ProductoID int'#10'declare' +
              ' @EstacionID int'#10'declare @Existencia int'#10'declare @Costo float'#10'de' +
              'clare @Precio float'#10#10'declare @Resultado varchar(50)'#10#10'select @Tur' +
              'noID = (Select :TurnoID)'#10'select @InvIni = (Select :InvIni)'#10'selec' +
              't @Resurtidos  = (Select :Resurtidos)'#10'select @InvFin = (Select :' +
              'InvFin)'#10'select @Venta = (Select :Venta)'#10'select @Bodega = (Select' +
              ' :Bodega)'#10'select @Fecha = (Select :Fecha)'#10'select @ProductoID = (' +
              'Select :ProductoID)'#10'select @EstacionID = (Select :EstacionID)'#10'se' +
              'lect @Existencia = (Select :Existencia)'#10'select @Costo = (Select ' +
              ':Costo)'#10'select @Precio = (Select :Precio)'#10#10'  if ((select count(*' +
              ') from dbo.LiquidacionProd where Fecha = @Fecha AND ProductoID =' +
              ' @ProductoID'#10'       AND EstacionID = @EstacionID) > 0)'#10'  begin'#10#10 +
              '    if (select CategoriaID from dbo.Producto where ProductoID = ' +
              '@ProductoID) = 2 or'#10'       (select CategoriaID from dbo.Producto' +
              ' where ProductoID = @ProductoID) = 3'#10'    begin'#10'    update dbo.Li' +
              'quidacionProd set'#10'           TurnoID = @TurnoID,'#10'           InvI' +
              'ni  = @InvIni,'#10'           Resurtidos = @Resurtidos,'#10'           I' +
              'nvFin = @InvFin,'#10'           Venta = @Venta,'#10'           Bodega = ' +
              '@Bodega,'#10'           Fecha = @Fecha,'#10'           ProductoID = @Pro' +
              'ductoID,'#10'           EstacionID = @EstacionID,'#10'           Existen' +
              'cia = @Existencia,'#10'           Costo = @Costo,'#10'           Precio ' +
              '= @Precio'#10'    where Fecha = @Fecha AND ProductoID = @ProductoID'#10 +
              '       AND EstacionID = @EstacionID'#10#10'    select @Resultado = '#39'Ac' +
              'tualizacion'#39#10'    end'#10'  end'#10#10'  else'#10'  begin'#10#10'    if (select Categ' +
              'oriaID from dbo.Producto where ProductoID = @ProductoID) = 2 or'#10 +
              '       (select CategoriaID from dbo.Producto where ProductoID = ' +
              '@ProductoID) = 3'#10'    begin'#10'    INSERT'#10'    INTO dbo.LiquidacionPr' +
              'od'#10'    (TurnoID, InvIni, Resurtidos, InvFin, Venta, Bodega, Fech' +
              'a, ProductoID, EstacionID, Existencia, Costo, Precio)'#10'    VALUES' +
              #10'    (@TurnoID, @InvIni, @Resurtidos, @InvFin, @Venta, @Bodega, ' +
              '@Fecha, @ProductoID, @EstacionID, @Existencia, @Costo, @Precio)'#10 +
              #10'    select @Resultado = '#39'Insercion'#39#10'    end'#10'  end'#10#10'Select @Resu' +
              'ltado as Resultado'#10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'Resultado'
                TableField = 'Resultado'
              end>
          end>
        Name = 'spLiquidacionProd'
        Fields = <
          item
            Name = 'Resultado'
            DataType = datString
            Size = 50
          end>
      end
      item
        Params = <
          item
            Name = 'Fecha'
            Value = '01/01/2010'
            ParamType = daptInput
          end
          item
            Name = 'Estacion'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'TurnoID'
            Value = '1'
            ParamType = daptInput
          end>
        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'Declare @FechaIni as Datetime'#10'Declare @FechaFin as datetime'#10'Decl' +
              'are @TurnoID as int'#10'Declare @Estacion as int'#10#10'Select @FechaIni =' +
              ' (Select :Fecha)'#10'Select @FechaFin = (Select :Fecha)'#10'Select @Esta' +
              'cion = (Select :Estacion)'#10'Select @TurnoID = (Select :TurnoID)'#10#10'D' +
              'ECLARE @ESTACIONID AS INT'#10'DECLARE @NOMBREESTACION AS VARCHAR (30' +
              ')'#10'DECLARE @PRODUCTOID AS INT'#10'DECLARE @CODIGOPRODUCTO AS VARCHAR(' +
              '30)'#10'DECLARE @PRODUCTONOMBRE AS VARCHAR(50)'#10'DECLARE @EXISTENCIA A' +
              'S INT'#10'DECLARE @VENTA AS INT'#10'DECLARE @RESUTIDOS AS INT'#10'DECLARE @P' +
              'RODIDALMACEN AS INT'#10'DECLARE @PRODIDALMACENCOD AS VARCHAR(50)'#10'DEC' +
              'LARE @PRODNOMALMACEN AS VARCHAR(50)'#10'DECLARE @INVINI AS INT'#10'DECLA' +
              'RE @PRECIOVENTA AS FLOAT'#10'DECLARE @COSTO AS FLOAT'#10'DECLARE @BODEGA' +
              ' AS INT'#10#10'CREATE TABLE #TEMP('#10'ESTACIONID INT,'#10'NOMBREESTACION VARC' +
              'HAR (30),'#10'PRODUCTOID INT,'#10'CODIGOPRODUCTO VARCHAR(30),'#10'PRODUCTONO' +
              'MBRE VARCHAR(50),'#10'EXISTENCIA INT,'#10'VENTA INT,'#10'RESUTIDOS INT,'#10'PROD' +
              'IDALMACEN INT,'#10'PRODIDALMACENCOD VARCHAR(50),'#10'PRODNOMALMACEN VARC' +
              'HAR(50),'#10'INVINI INT,'#10'PRECIOVENTA FLOAT,'#10'COSTO FLOAT,'#10'BODEGA INT'#10 +
              ')'#10#10'DECLARE prueba_Cursor SCROLL CURSOR FOR'#10'SELECT  IsNull(A.Esta' +
              'cionID, IsNull(B.EstacionID, IsNull(C.EstacionID, isNull(D.Estac' +
              'ionID, isNull(E.EstacionID,0))))) as EstacionID,'#10'           IsNu' +
              'll(A.Estacion, IsNull(B.Nombre, IsNull(C.Nombre, isNull(D.Nombre' +
              ', isNull(E.Nombre,0))))) as NombreEstacion,'#10'           IsNull(Is' +
              'Null(B.ProdIDVenta,ProdIDAlmacen),0) as ProductoID,'#10'           I' +
              'sNull(IsNull(B.ProdCodVenta,ProdIDAlmacenCod),0) as CodigoProduc' +
              'to,'#10'           IsNull(IsNull(B.NomProdVenta, ProdNomAlmacen),0) ' +
              'as ProductoNombre,'#10'           A.Existencia,'#10'           SUM(B.Ven' +
              'ta) AS Venta , C.Resurtidos,'#10'           A.ProdIDAlmacen, A.ProdI' +
              'DAlmacenCod, A.ProdNomAlmacen, ISNULL(D.InvIni, 0)as InvIni,'#10'   ' +
              '        IsNull(A.PrecioVenta,0) as PrecioVenta, IsNull(B.Costo, ' +
              'A.Costo) as Costo,'#10'           E.Bodega'#10'FROM (SELECT Estacion.Est' +
              'acionID, Estacion.Nombre AS Estacion, Producto.ProductoID AS Pro' +
              'dIDAlmacen, Producto.Codigo as ProdIDAlmacenCod,'#10'             SU' +
              'M(DetalleMovimientoAlmacen2.Cantidad * TipoMovimientoAlmacen2.Op' +
              'erador) AS Existencia, Producto.Nombre as ProdNomAlmacen,'#10'      ' +
              '       Producto.PrecioVenta, Producto.Costo'#10'             FROM   ' +
              '       Almacen2 INNER JOIN'#10'             MovimientoAlmacen2 ON Al' +
              'macen2.AlmacenID = MovimientoAlmacen2.AlmacenID INNER JOIN'#10'     ' +
              '        DetalleMovimientoAlmacen2 ON'#10'             MovimientoAlma' +
              'cen2.MovimientoAlmacenID = DetalleMovimientoAlmacen2.MovimientoA' +
              'lmacenID INNER JOIN'#10'             TipoMovimientoAlmacen2 ON'#10'     ' +
              '        MovimientoAlmacen2.TipoMovimientoAlmacenID = TipoMovimie' +
              'ntoAlmacen2.TipoMovimientoAlmacenID INNER JOIN'#10'             Prod' +
              'ucto ON DetalleMovimientoAlmacen2.ProductoID = Producto.Producto' +
              'ID INNER JOIN'#10'             Estacion ON MovimientoAlmacen2.Estaci' +
              'onID = Estacion.EstacionID'#10'WHERE      (MovimientoAlmacen2.Estaci' +
              'onID = @Estacion AND MovimientoAlmacen2.Fecha <= @FechaFin) and ' +
              'Producto.ProductoID > 3'#10'            GROUP BY Producto.Nombre, Pr' +
              'oducto.ProductoID, Producto.Codigo, Estacion.EstacionID, Estacio' +
              'n.Nombre, Producto.PrecioVenta, Producto.Costo)'#10'            A FU' +
              'LL JOIN'#10#10'       (SELECT    DISTINCT DetalleLiquidacion2.Producto' +
              'ID as ProdIDVenta, SUM(DetalleLiquidacion2.Cantidad) AS Venta,'#10' ' +
              '                 Producto.Codigo as ProdCodVenta, Producto.Nombr' +
              'e as NomProdVenta, Estacion.EstacionID, Estacion.Nombre,'#10'       ' +
              '           DetalleLiquidacion2.Importe, IsNull(DetalleLiquidacio' +
              'n2.Costo,Producto.Costo) as Costo'#10'       FROM      DetalleLiquid' +
              'acion2 INNER JOIN'#10'          DespachadorLiquidacion2 ON DetalleLi' +
              'quidacion2.DespachadorLiquidacionID = DespachadorLiquidacion2.De' +
              'spachadorLiquidacionID INNER JOIN'#10'          Liquidacion2 ON'#10'    ' +
              '      DespachadorLiquidacion2.LiquidacionID = Liquidacion2.Liqui' +
              'dacionID INNER JOIN'#10'          Agrupacion2 ON DespachadorLiquidac' +
              'ion2.AgrupacionID = Agrupacion2.AgrupacionID INNER JOIN'#10'        ' +
              '  Almacen2 ON Almacen2.AlmacenID = Agrupacion2.AlmacenID INNER J' +
              'OIN'#10'          Producto ON DetalleLiquidacion2.ProductoID = Produ' +
              'cto.ProductoID INNER JOIN'#10'          Estacion ON Liquidacion2.Est' +
              'acionID = Estacion.EstacionID'#10'       WHERE  (Agrupacion2.Estacio' +
              'nID = @Estacion)'#10'          AND (Liquidacion2.Fecha Between @Fech' +
              'aIni and @FechaFin)  and Producto.ProductoID > 3'#10'       GROUP BY' +
              ' DetalleLiquidacion2.ProductoID, Estacion.EstacionID, Estacion.N' +
              'ombre, DetalleLiquidacion2.Importe, DetalleLiquidacion2.Costo,'#10' ' +
              '      Producto.Codigo, Producto.Nombre, liquidacion2.cerrada, Pr' +
              'oducto.Costo) B ON A.ProdIDAlmacen = B.ProdIDVenta'#10'       FULL J' +
              'OIN'#10#10'      (Select SUM(DetalleMovimientoAlmacen2.Cantidad) as Re' +
              'surtidos, Producto.ProductoID, Estacion.EstacionID, Estacion.Nom' +
              'bre From'#10'        Almacen2 INNER JOIN'#10'        MovimientoAlmacen2 ' +
              'ON Almacen2.AlmacenID = MovimientoAlmacen2.AlmacenID INNER JOIN'#10 +
              '        DetalleMovimientoAlmacen2 ON'#10'        MovimientoAlmacen2.' +
              'MovimientoAlmacenID = DetalleMovimientoAlmacen2.MovimientoAlmace' +
              'nID INNER JOIN'#10'        TipoMovimientoAlmacen2 ON'#10'        Movimie' +
              'ntoAlmacen2.TipoMovimientoAlmacenID = TipoMovimientoAlmacen2.Tip' +
              'oMovimientoAlmacenID INNER JOIN'#10'        Producto ON DetalleMovim' +
              'ientoAlmacen2.ProductoID = Producto.ProductoID INNER JOIN'#10'      ' +
              '  Estacion ON MovimientoAlmacen2.EstacionID = Estacion.EstacionI' +
              'D'#10'        WHERE (MovimientoAlmacen2.EstacionID = @Estacion AND M' +
              'ovimientoAlmacen2.Fecha Between @FechaIni AND @FechaFin'#10'        ' +
              'AND TipoMovimientoAlmacen2.Operador > -1) and (MovimientoAlmacen' +
              '2.TipoMovimientoAlmacenID = 4)'#10'        GROUP BY Producto.Product' +
              'oID, Estacion.EstacionID, Estacion.Nombre) C ON'#10'        A.ProdID' +
              'Almacen = C.ProductoID'#10'       LEFT OUTER JOIN'#10#10'      (Select SUM' +
              '(DetalleMovimientoAlmacen2.Cantidad * TipoMovimientoAlmacen2.Ope' +
              'rador) as InvIni, Producto.ProductoID,'#10'      Estacion.EstacionID' +
              ', Estacion.Nombre From'#10'        Almacen2 INNER JOIN'#10'        Movim' +
              'ientoAlmacen2 ON Almacen2.AlmacenID = MovimientoAlmacen2.Almacen' +
              'ID INNER JOIN'#10'        DetalleMovimientoAlmacen2 ON'#10'        Movim' +
              'ientoAlmacen2.MovimientoAlmacenID = DetalleMovimientoAlmacen2.Mo' +
              'vimientoAlmacenID INNER JOIN'#10'        TipoMovimientoAlmacen2 ON'#10' ' +
              '       MovimientoAlmacen2.TipoMovimientoAlmacenID = TipoMovimien' +
              'toAlmacen2.TipoMovimientoAlmacenID INNER JOIN'#10'        Producto O' +
              'N DetalleMovimientoAlmacen2.ProductoID = Producto.ProductoID INN' +
              'ER JOIN'#10'        Estacion ON MovimientoAlmacen2.EstacionID = Esta' +
              'cion.EstacionID'#10'        WHERE (MovimientoAlmacen2.EstacionID = @' +
              'Estacion AND MovimientoAlmacen2.Fecha <= @FechaFin)'#10'        AND ' +
              '(Almacen2.Compras <> 1)'#10'        GROUP BY Producto.ProductoID, Es' +
              'tacion.EstacionID, Estacion.Nombre) D ON'#10'        A.ProdIDAlmacen' +
              ' = D.ProductoID'#10'        LEFT OUTER JOIN'#10#10'       (Select SUM(Deta' +
              'lleMovimientoAlmacen2.Cantidad * TipoMovimientoAlmacen2.Operador' +
              ') as Bodega, Producto.ProductoID,'#10'        Estacion.EstacionID, E' +
              'stacion.Nombre From'#10'        Almacen2 INNER JOIN'#10'        Movimien' +
              'toAlmacen2 ON Almacen2.AlmacenID = MovimientoAlmacen2.AlmacenID ' +
              'INNER JOIN'#10'        DetalleMovimientoAlmacen2 ON'#10'        Movimien' +
              'toAlmacen2.MovimientoAlmacenID = DetalleMovimientoAlmacen2.Movim' +
              'ientoAlmacenID INNER JOIN'#10'        TipoMovimientoAlmacen2 ON'#10'    ' +
              '    MovimientoAlmacen2.TipoMovimientoAlmacenID = TipoMovimientoA' +
              'lmacen2.TipoMovimientoAlmacenID INNER JOIN'#10'        Producto ON D' +
              'etalleMovimientoAlmacen2.ProductoID = Producto.ProductoID INNER ' +
              'JOIN'#10'        Estacion ON MovimientoAlmacen2.EstacionID = Estacio' +
              'n.EstacionID'#10'        WHERE (MovimientoAlmacen2.EstacionID = @Est' +
              'acion AND MovimientoAlmacen2.Fecha <= @FechaFin'#10'        AND Alma' +
              'cen2.Compras > 0)'#10'        GROUP BY Producto.ProductoID, Estacion' +
              '.EstacionID, Estacion.Nombre) E ON'#10'        A.ProdIDAlmacen = E.P' +
              'roductoID'#10'GROUP BY A.Estacion, B.EstacionID, C.EstacionID, D.Est' +
              'acionID, E.EstacionID, A.EstacionID,'#10'         B.Nombre, C.Nombre' +
              ', D.Nombre, E.Nombre, B.ProdIDVenta, A.ProdIDAlmacenCod, B.ProdC' +
              'odVenta,'#10'         A.ProdIDAlmacen, B.NomProdVenta, A.ProdNomAlma' +
              'cen, A.Existencia, C.Resurtidos, D.InvIni,'#10'         A.PrecioVent' +
              'a, B.Importe, A.Costo, B.Costo, E.Bodega'#10#10'OPEN prueba_Cursor'#10'FET' +
              'CH NEXT FROM prueba_Cursor'#10'into @ESTACIONID, @NOMBREESTACION, @P' +
              'RODUCTOID, @CODIGOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCIA,'#10'     @' +
              'VENTA, @RESUTIDOS, @PRODIDALMACEN, @PRODIDALMACENCOD, @PRODNOMAL' +
              'MACEN, @INVINI, @PRECIOVENTA,'#10'     @COSTO, @BODEGA'#10'WHILE @@FETCH' +
              '_STATUS = 0'#10'BEGIN'#10'  INSERT INTO #TEMP (ESTACIONID,NOMBREESTACION' +
              ',PRODUCTOID,CODIGOPRODUCTO,PRODUCTONOMBRE,EXISTENCIA,VENTA,RESUT' +
              'IDOS,PRODIDALMACEN,PRODIDALMACENCOD,'#10'                   PRODNOMA' +
              'LMACEN,INVINI,PRECIOVENTA,COSTO,BODEGA)VALUES(@ESTACIONID, @NOMB' +
              'REESTACION, @PRODUCTOID, @CODIGOPRODUCTO, @PRODUCTONOMBRE, @EXIS' +
              'TENCIA,'#10'     @VENTA, @RESUTIDOS, @PRODIDALMACEN, @PRODIDALMACENC' +
              'OD, @PRODNOMALMACEN, @INVINI, @PRECIOVENTA,'#10'     @COSTO, @BODEGA' +
              ')'#10#10'  FETCH NEXT FROM prueba_Cursor into @ESTACIONID, @NOMBREESTA' +
              'CION, @PRODUCTOID, @CODIGOPRODUCTO, @PRODUCTONOMBRE, @EXISTENCIA' +
              ','#10'     @VENTA, @RESUTIDOS, @PRODIDALMACEN, @PRODIDALMACENCOD, @P' +
              'RODNOMALMACEN, @INVINI, @PRECIOVENTA,'#10'     @COSTO, @BODEGA'#10'END'#10#10 +
              'CLOSE prueba_Cursor'#10'DEALLOCATE prueba_Cursor'#10#10'SELECT SUM(VENTA) ' +
              'AS VENTA, ESTACIONID,NOMBREESTACION,PRODUCTOID,CODIGOPRODUCTO,PR' +
              'ODUCTONOMBRE,EXISTENCIA,RESUTIDOS as RESURTIDOS,PRODIDALMACEN,PR' +
              'ODIDALMACENCOD,'#10'         PRODNOMALMACEN,INVINI,PRECIOVENTA,COSTO' +
              ',BODEGA FROM #TEMP'#10'GROUP BY ESTACIONID,NOMBREESTACION,PRODUCTOID' +
              ',CODIGOPRODUCTO,PRODUCTONOMBRE,EXISTENCIA,RESUTIDOS,PRODIDALMACE' +
              'N,PRODIDALMACENCOD,'#10'         PRODNOMALMACEN,INVINI,PRECIOVENTA,C' +
              'OSTO,BODEGA'#10'ORDER BY PRODUCTOID ASC'#10#10'DROP TABLE #TEMP'#10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'VENTA'
                TableField = 'VENTA'
              end
              item
                DatasetField = 'ESTACIONID'
                TableField = 'ESTACIONID'
              end
              item
                DatasetField = 'NOMBREESTACION'
                TableField = 'NOMBREESTACION'
              end
              item
                DatasetField = 'PRODUCTOID'
                TableField = 'PRODUCTOID'
              end
              item
                DatasetField = 'CODIGOPRODUCTO'
                TableField = 'CODIGOPRODUCTO'
              end
              item
                DatasetField = 'PRODUCTONOMBRE'
                TableField = 'PRODUCTONOMBRE'
              end
              item
                DatasetField = 'EXISTENCIA'
                TableField = 'EXISTENCIA'
              end
              item
                DatasetField = 'RESURTIDOS'
                TableField = 'RESURTIDOS'
              end
              item
                DatasetField = 'PRODIDALMACEN'
                TableField = 'PRODIDALMACEN'
              end
              item
                DatasetField = 'PRODIDALMACENCOD'
                TableField = 'PRODIDALMACENCOD'
              end
              item
                DatasetField = 'PRODNOMALMACEN'
                TableField = 'PRODNOMALMACEN'
              end
              item
                DatasetField = 'INVINI'
                TableField = 'INVINI'
              end
              item
                DatasetField = 'PRECIOVENTA'
                TableField = 'PRECIOVENTA'
              end
              item
                DatasetField = 'COSTO'
                TableField = 'COSTO'
              end
              item
                DatasetField = 'BODEGA'
                TableField = 'BODEGA'
              end>
          end>
        Name = 'spObtenEstacionLiq'
        Fields = <
          item
            Name = 'VENTA'
            DataType = datInteger
          end
          item
            Name = 'ESTACIONID'
            DataType = datInteger
          end
          item
            Name = 'NOMBREESTACION'
            DataType = datString
            Size = 30
          end
          item
            Name = 'PRODUCTOID'
            DataType = datInteger
          end
          item
            Name = 'CODIGOPRODUCTO'
            DataType = datString
            Size = 30
          end
          item
            Name = 'PRODUCTONOMBRE'
            DataType = datString
            Size = 50
          end
          item
            Name = 'EXISTENCIA'
            DataType = datInteger
          end
          item
            Name = 'RESURTIDOS'
            DataType = datInteger
          end
          item
            Name = 'PRODIDALMACEN'
            DataType = datInteger
          end
