              item
                DatasetField = 'Barras'
                TableField = 'Barras'
              end
              item
                DatasetField = 'CuentaContableCompra'
                TableField = 'CuentaContableCompra'
              end
              item
                DatasetField = 'CuentaContableVenta'
                TableField = 'CuentaContableVenta'
              end
              item
                DatasetField = 'Inventariable'
                TableField = 'Inventariable'
              end
              item
                DatasetField = 'CategoriaID'
                TableField = 'CategoriaID'
              end
              item
                DatasetField = 'Puntos'
