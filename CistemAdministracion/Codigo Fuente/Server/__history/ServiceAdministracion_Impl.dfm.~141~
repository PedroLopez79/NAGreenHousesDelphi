        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'SELECT     Liquidacion2.EstacionID, Agrupacion2.AlmacenID, Estac' +
              'ion.Impuesto AS ImpuestoPorcentaje, SUM(CAST(DetalleLiquidacion2' +
              '.Importe AS DECIMAL(10,2))) AS Importe,'#10'                      SU' +
              'M(CAST(DetalleLiquidacion2.Importe / (1 + (Estacion.Impuesto / 1' +
              '00)) AS Decimal(10,2))) AS SubTotal,'#10#9#9'      SUM(CAST(DetalleLiq' +
              'uidacion2.Importe AS DECIMAL(10,2)) - CAST(DetalleLiquidacion2.I' +
              'mporte / (1 + (Estacion.Impuesto / 100)) AS Decimal(10,2))) AS I' +
              'mpuesto'#10'FROM         DetalleLiquidacion2 INNER JOIN'#10'            ' +
              '          DespachadorLiquidacion2 ON DetalleLiquidacion2.Despach' +
              'adorLiquidacionID = DespachadorLiquidacion2.DespachadorLiquidaci' +
              'onID INNER JOIN'#10'                      Agrupacion2 ON Despachador' +
