              'dorID, C.BombaID, C.ProductoID,'#10'    C.AgrupacionID, LiquidacionI' +
              'D,ImporteCupones,NumeroCupones,'#10'    ImporteTarjetas,NumeroTarjet' +
              'as,ImporteNotas,NumeroNotas'#10'  FROM'#10'    dbo.Corte C --inner join ' +
              'AgrupacionBomba A on C.AgrupacionID=A.AgrupacionID and c.BombaID' +
              ' = A.BombaID'#10'    '#10'  Where C.Secuencia=@Secuencia and C.Agrupacio' +
              'nID = @AgrupacionID'#10
            StatementType = stSQL
