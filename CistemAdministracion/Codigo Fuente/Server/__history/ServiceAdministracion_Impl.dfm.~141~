              ' (SELECT  top 1   descripcion as Premio, cast(@puntos / Puntos a' +
              's int) as Total'#10'FROM         PuntosPremio'#10'where puntos <= @punto' +
              's'#10'order by puntos desc) A'#10'*/'#10#10'SELECT '#39'('#39' + CAST(Total AS VARCHAR' +
              '(5)) + '#39') - '#39' + Premio AS Premio'#10'FROM ( SELECT TOP 1 PuntosPremi' +
              'o.Descripcion Premio, CAST(:Puntos / Puntos AS INT) Total'#10'      ' +
              ' FROM PuntosPremio'#10'       INNER JOIN DetalleAgrupacionEstacion D' +
              'AE ON DAE.EstacionID = :EstacionID'#10'       INNER JOIN AgrupacionE' +
