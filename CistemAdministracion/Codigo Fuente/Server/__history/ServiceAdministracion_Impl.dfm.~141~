        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'declare @EstacionID int'#10#10'select @EstacionID = (select :EstacionI' +
              'D)'#10#10'SELECT     ProductoID, Codigo, Nombre, CAST(CASE WHEN Precio' +
              ' > 0 THEN Precio ELSE PrecioVenta END AS DECIMAL(15,4)) AS Preci' +
              'oVenta, ClavePemex, Costo, Status, Barras, CuentaContableCompra,' +
              ' CuentaContableVenta, Inventariable,'#10'                      Costo' +
              'Promedio, CategoriaID, IEPS'#10'FROM         (SELECT     Producto.Pr' +
              'oductoID, Producto.Codigo, Producto.Nombre, Producto.PrecioVenta' +
              ', Producto.ClavePemex, Producto.Costo, Producto.Status,'#10'        ' +
              '              Producto.Barras, Producto.CuentaContableCompra, Pr' +
              'oducto.CuentaContableVenta, Producto.Inventariable, Producto.Cos' +
              'toPromedio,'#10'                      Producto.CategoriaID, Producto' +
              '.IEPS, ProductoPrecio.Precio'#10'                       FROM        ' +
              '  Producto LEFT OUTER JOIN'#10'                                     ' +
              '             (SELECT     *'#10'                                     ' +
              '               FROM          ProductoPrecio'#10'                    ' +
              '                                WHERE      estacionid = @Estacio' +
              'nID) ProductoPrecio ON ProductoPrecio.ProductoID = Producto.Prod' +
              'uctoID) A'#10
            StatementType = stSQL
            ColumnMappings = <
