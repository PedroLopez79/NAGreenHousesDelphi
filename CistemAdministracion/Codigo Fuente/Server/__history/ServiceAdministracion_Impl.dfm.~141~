        Params = <
          item
            Name = 'EstacionID'
            Value = ''
            ParamType = daptInput
          end
          item
            Name = 'ClienteID'
            Value = ''
            ParamType = daptInput
          end
          item
            Name = 'Ticket'
            Value = ''
            ParamType = daptInput
          end>
        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'declare @ClienteID as int'#10'declare @EstacionID as int'#10'declare @Ti' +
              'cket as int'#10#10'select @EstacionID = (Select :EstacionID)'#10'select @C' +
              'lienteID = (Select :ClienteID)'#10'select @Ticket = (select :Ticket)' +
              #10#10'SELECT     ConsumoExpress.Precio, SUM(ConsumoExpress.Volumen) ' +
              'AS Volumen, SUM(ConsumoExpress.Importe) AS Importe, ConsumoExpre' +
              'ss.ProductoID,'#10'                      Producto.Codigo, Producto.N' +
              'ombre'#10'FROM         ConsumoExpress INNER JOIN'#10'                   ' +
              '   Producto ON ConsumoExpress.ProductoID = Producto.ProductoID'#10'W' +
              'HERE     (ConsumoExpress.ClienteID = @ClienteID) AND (ConsumoExp' +
              'ress.EstacionID = @EstacionID) AND (ConsumoExpress.Facturado = 0' +
              ') AND'#10'          ((@Ticket = 0) or ((@Ticket > 0) and (ConsumoExp' +
              'ress.Secuencia = @Ticket)))'#10'GROUP BY ConsumoExpress.Precio, Cons' +
              'umoExpress.ProductoID, Producto.Codigo, Producto.Nombre'#10#10#10'Update' +
              ' ConsumoExpress Set Facturado=1'#10'Where (ClienteID = @ClienteID) a' +
              'nd (EstacionID = @EstacionID) and (Facturado = 0) and'#10'      ((@T' +
              'icket = 0) or ((@Ticket > 0) and (Secuencia = @Ticket)))'#10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'Precio'
                TableField = 'precio'
              end
              item
                DatasetField = 'Volumen'
                TableField = 'Volumen'
              end
              item
                DatasetField = 'Importe'
                TableField = 'Importe'
