            SQL = 
              'declare @Estacionid int'#10'declare @Turnoid int'#10'declare @BombaID in' +
              't'#10#10'declare @Liquidacionid int'#10'declare @Agrupacionid int'#10#10'select ' +
              '@estacionid = (select :EstacionID)'#10'select @turnoid = (select :Tu' +
              'rnoID)'#10'select @BombaID = (select :BombaID)'#10#10'select @Liquidacioni' +
              'd = (select liquidacionid from liquidacion2 where estacionid=@es' +
              'tacionid and turnoid=@turnoid)'#10'select @agrupacionid = (SELECT   ' +
              '  Agrupacion2.AgrupacionID'#10'FROM         Agrupacion2 INNER JOIN'#10' ' +
              '                     AgrupacionBomba2 ON Agrupacion2.AgrupacionI' +
              'D = AgrupacionBomba2.AgrupacionID'#10'WHERE     (AgrupacionBomba2.Bo' +
              'mbaID = @bombaid) AND (Agrupacion2.EstacionID = @estacionid))'#10#10#10 +
              'INSERT'#10'  INTO dbo.DespachadorLiquidacion2'#10'    (DespachadorLiquid' +
              'acionID, DespachadorID, Importe, Entregado,'#10'     Diferencia, Liq' +
              'uidacionID, AgrupacionID)'#10'  VALUES'#10'    (:DespachadorLiquidacionI' +
              'D, 0, 0,'#10'     0, 0, @LiquidacionID, @AgrupacionID)'#10#10
            StatementType = stSQL
