              'DECLARE @CAD AS VARCHAR(4000)'#10'DECLARE @SUCURSAL AS INT'#10#10'SELECT @' +
              'CAD = (SELECT '#39#39')'#10#10'SELECT @SUCURSAL = ISNULL((SELECT TOP 1 ISNUL' +
              'L(SUCURSALDE,0) FROM DatosSATEmpresas WHERE EmpresaID=:EstacionI' +
              'D AND ISNULL(SUCURSAL,0) = 1),-1)'#10#10'IF @SUCURSAL > 0'#10#10'SELECT FACT' +
              'URA.SERIE,RTRIM(CAST(FACTURA.FOLIO AS VARCHAR(50))) AS FOLIO,'#10'  ' +
              '     LEFT(CONVERT(VARCHAR, FACTURA.FECHA, 120), 10)+'#39'T'#39'+'#10'       ' +
              'Substring(convert(varchar, FACTURA.FECHA, 14),1,8) AS FECHA,LTRI' +
              'M(CAST(DatosSATEmpresas.NOAPROBACION AS VARCHAR (10))) AS NOAPRO' +
              'BACION,'#10'       CAST(YEAR(FACTURA.FECHA) AS VARCHAR(4)) AS ANOAPR' +
              'OBACION, FORMAPAGO.DESCRIPCION AS FORMAPAGO,'#10'       CONVERT(VARC' +
              'HAR,CONVERT(MONEY,FACTURA.SUBTOTAL),1) AS SUBTOTAL,CONVERT(VARCH' +
              'AR,CONVERT(MONEY,FACTURA.TOTAL),1) AS TOTAL,'#10'       DatosSATEmpr' +
              'esas.RFC AS RFCEMISOR, DatosSATEmpresas.NOMBRE AS NOMEMPRESAEMIS' +
              'OR, DatosSATEmpresas.DOMICILIO AS DOMEMISOR, ISNULL(DatosSATEmpr' +
              'esas.NOEXTERIOR,'#39#39') NOEXTERIOREMISOR,'#10'       ISNULL(DatosSATEmpr' +
              'esas.COLONIA,'#39#39') AS COLONIAEMISOR,ISNULL(DatosSATEmpresas.MUNICI' +
              'PIO,'#39#39') AS MUNICIPIOEMISOR,ISNULL(DatosSATEmpresas.ESTADO,'#39#39') AS' +
              ' ESTADOEMISOR,'#10'       ISNULL(DatosSATEmpresas.PAIS,'#39#39') AS PAISEM' +
              'ISOR,ISNULL(DatosSATEmpresas.CODIGOPOSTAL,'#39#39') AS CODIGOPOSTALEMI' +
              'SOR,'#10'       ISNULL(CLIENTE.RFC,'#39#39') AS RFCRECEPTOR, ISNULL(CLIENT' +
              'E.NOMBRE,'#39#39') AS NOMBREEMPRECEPTOR, ISNULL(CLIENTE.CALLE,'#39#39') AS D' +
              'OMICILIORECEPTOR, ISNULL(CLIENTE.NOEXTERIOR,'#39#39') AS NOEXTERIORREC' +
              'EPTOR,'#10'       ISNULL(CLIENTE.COLONIA,'#39#39') AS COLONIARECEPTOR,ISNU' +
              'LL(CLIENTE.LOCALIDAD,'#39#39') AS LOCALIDADRECEPTOR,ISNULL(CLIENTE.MUN' +
              'ICIPIO,'#39#39') AS MUNICIPIORECEPTOR,'#10'       ISNULL(CLIENTE.ESTADO,'#39#39 +
              ') AS ESTADORECEPTOR,ISNULL(CLIENTE.PAIS,'#39#39') AS PAISRECEPTOR,ISNU' +
              'LL(CAST(CLIENTE.CODIGOPOSTAL AS VARCHAR(20)),'#39#39') AS CODIGOPOSTAL' +
              'RECEPTOR,'#10'       ISNULL(CLIENTE.EMAIL,'#39#39') AS EMAIL'#10'FROM FACTURA ' +
              'INNER JOIN CLIENTE ON CLIENTE.CLIENTEID = FACTURA.CLIENTEID'#10'INNE' +
              'R JOIN DatosSATEmpresas ON DatosSATEmpresas.DatosSATEmpresasID =' +
              ' DatosSATEmpresas.DatosSATEmpresasID'#10'INNER JOIN FORMAPAGO ON FAC' +
              'TURA.FORMAPAGOID = FORMAPAGO.FORMAPAGOID'#10'WHERE FACTURAID = :Fact' +
              'uraID AND DatosSATEmpresas.EmpresaID = @SUCURSAL AND ISNULL(SUCU' +
              'RSAL,0)= 0'#10#10'ELSE'#10#10'SELECT FACTURA.SERIE,RTRIM(CAST(FACTURA.FOLIO ' +
              'AS VARCHAR(50))) AS FOLIO,'#10'       LEFT(CONVERT(VARCHAR, FACTURA.' +
              'FECHA, 120), 10)+'#10'       '#39'T'#39'+'#10#10'       Substring(convert(varchar,' +
              ' FACTURA.FECHA, 14),1,8) AS FECHA,LTRIM(CAST(DatosSATEmpresas.NO' +
              'APROBACION AS VARCHAR (10))) AS NOAPROBACION,'#10'       CAST(YEAR(F' +
              'ACTURA.FECHA) AS VARCHAR(4)) AS ANOAPROBACION, FORMAPAGO.DESCRIP' +
              'CION AS FORMAPAGO,'#10'       CONVERT(VARCHAR,CONVERT(MONEY,FACTURA.' +
              'SUBTOTAL),1) AS SUBTOTAL,CONVERT(VARCHAR,CONVERT(MONEY,FACTURA.T' +
              'OTAL),1) AS TOTAL,'#10'       DatosSATEmpresas.RFC AS RFCEMISOR, Dat' +
              'osSATEmpresas.NOMBRE AS NOMEMPRESAEMISOR, DatosSATEmpresas.DOMIC' +
              'ILIO AS DOMEMISOR, ISNULL(DatosSATEmpresas.NOEXTERIOR,'#39#39') NOEXTE' +
              'RIOREMISOR,'#10'       ISNULL(DatosSATEmpresas.COLONIA,'#39#39') AS COLONI' +
              'AEMISOR,ISNULL(DatosSATEmpresas.MUNICIPIO,'#39#39') AS MUNICIPIOEMISOR' +
              ',ISNULL(DatosSATEmpresas.ESTADO,'#39#39') AS ESTADOEMISOR,'#10'       ISNU' +
              'LL(DatosSATEmpresas.PAIS,'#39#39') AS PAISEMISOR,ISNULL(DatosSATEmpres' +
              'as.CODIGOPOSTAL,'#39#39') AS CODIGOPOSTALEMISOR,'#10'       ISNULL(CLIENTE' +
              '.RFC,'#39#39') AS RFCRECEPTOR, ISNULL(CLIENTE.NOMBRE,'#39#39') AS NOMBREEMPR' +
              'ECEPTOR, ISNULL(CLIENTE.CALLE,'#39#39') AS DOMICILIORECEPTOR, ISNULL(C' +
              'LIENTE.NOEXTERIOR,'#39#39') AS NOEXTERIORRECEPTOR,'#10'       ISNULL(CLIEN' +
              'TE.COLONIA,'#39#39') AS COLONIARECEPTOR,ISNULL(CLIENTE.LOCALIDAD,'#39#39') A' +
              'S LOCALIDADRECEPTOR,ISNULL(CLIENTE.MUNICIPIO,'#39#39') AS MUNICIPIOREC' +
              'EPTOR,'#10'       ISNULL(CLIENTE.ESTADO,'#39#39') AS ESTADORECEPTOR,ISNULL' +
              '(CLIENTE.PAIS,'#39#39') AS PAISRECEPTOR,ISNULL(CAST(CLIENTE.CODIGOPOST' +
              'AL AS VARCHAR(20)),'#39#39') AS CODIGOPOSTALRECEPTOR,'#10'       ISNULL(CL' +
              'IENTE.EMAIL,'#39#39') AS EMAIL'#10'FROM FACTURA INNER JOIN CLIENTE ON CLIE' +
              'NTE.CLIENTEID = FACTURA.CLIENTEID'#10'INNER JOIN DatosSATEmpresas ON' +
              ' DatosSATEmpresas.DatosSATEmpresasID = DatosSATEmpresas.DatosSAT' +
              'EmpresasID'#10'INNER JOIN FORMAPAGO ON FACTURA.FORMAPAGOID = FORMAPA' +
              'GO.FORMAPAGOID'#10'WHERE FACTURAID = :FacturaID AND DatosSATEmpresas' +
              '.EmpresaID = :EstacionID AND ISNULL(SUCURSAL,0)= 0'#10
            StatementType = stSQL
            ColumnMappings = <
