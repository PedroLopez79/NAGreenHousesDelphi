            Name = 'Administracion'
            SQL = 
              'SELECT     Agrupacion2.AlmacenID, DetalleLiquidacion2.ProductoID' +
              ', SUM(CAST(DetalleLiquidacion2.Cantidad AS DECIMAL(10, 2))) AS C' +
              'antidad, Sum(CAST(DetalleLiquidacion2.Importe AS DECIMAL(10, 2))' +
              ') AS Importe,'#10'           SUM(CAST(CAST(DetalleLiquidacion2.Impor' +
              'te AS DECIMAL(10, 2)) / CAST(DetalleLiquidacion2.Cantidad AS DEC' +
              'IMAL(10, 2)) AS DECIMAL(10, 2))) AS Precio'#10'FROM         DetalleL' +
              'iquidacion2 INNER JOIN'#10'                      DespachadorLiquidac' +
              'ion2 ON DetalleLiquidacion2.DespachadorLiquidacionID = Despachad' +
