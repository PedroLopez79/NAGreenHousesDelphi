        Params = <
          item
            Name = 'EJERCICIO'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'FIN'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'INI'
            Value = '1'
            ParamType = daptInput
          end
          item
            Name = 'DESPACHADORID'
            Value = '1'
            ParamType = daptInput
          end>
        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'DECLARE @EJERCICIO INT'#10'DECLARE @FIN INT'#10'DECLARE @INI INT'#10'DECLARE' +
              ' @DESPACHADORID INT'#10#10'DECLARE @FECHA DATETIME'#10'DECLARE @FECHA2 DAT' +
              'ETIME'#10'DECLARE @REFERENCIA VARCHAR(100)'#10'DECLARE @CARGO INT'#10'DECLAR' +
              'E @ABONO INT'#10'DECLARE @SALDO INT'#10'DECLARE @SALDOTOTAL INT'#10#10'SET @SA' +
              'LDOTOTAL = 0'#10#10'CREATE TABLE #TEMP'#10'    (FECHA DATETIME,'#10'     REFER' +
              'ENCIA VARCHAR(100),'#10'     CARGO INT,'#10'     ABONO INT,'#10'     SALDO I' +
              'NT)'#10#10'SET @EJERCICIO = (SELECT :EJERCICIO)'#10'SET @FIN = (SELECT :FI' +
              'N)'#10'SET @INI = (SELECT :INI)'#10'SET @DESPACHADORID = (SELECT :DESPAC' +
              'HADORID)'#10'SET @FECHA2 = CAST((SELECT CAST(@INI AS VARCHAR(02))+'#39'/' +
              '01/'#39'+CAST(@EJERCICIO AS VARCHAR(04)))AS DATETIME)'#10#10'DECLARE prueb' +
              'a_Cursor SCROLL CURSOR FOR'#10'SELECT FECHA, REFERENCIA, SUM(CARGO) ' +
              'AS CARGO, SUM(ABONO) AS ABONO, (SUM(CARGO)-SUM(ABONO))AS SALDO'#10'F' +
              'ROM MOVIMIENTODESPACHADOR WHERE ((ejercicio = @ejercicio and per' +
              'iodo >= @ini and periodo <= @fin))'#10'AND (DESPACHADORID = @DESPACH' +
              'ADORID)'#10'GROUP BY FECHA, REFERENCIA'#10'ORDER BY FECHA ASC'#10#10'SET @SALD' +
              'OTOTAL = ISNULL((SELECT (SUM(CARGO)-SUM(ABONO))AS SALDO'#10'        ' +
              '           FROM MOVIMIENTODESPACHADOR WHERE ((ejercicio < @Ejerc' +
              'icio) or (ejercicio = @ejercicio and periodo < @ini))'#10'          ' +
              '         AND (DESPACHADORID = @DESPACHADORID)),0)'#10#10'INSERT INTO #' +
              'TEMP(FECHA, REFERENCIA, CARGO, ABONO, SALDO)VALUES(@FECHA2,'#39'-=SA' +
              'LDO INICIAL=-'#39', @SALDOTOTAL,0,@SALDOTOTAL)'#10#10'OPEN prueba_Cursor'#10'F' +
              'ETCH NEXT FROM prueba_Cursor'#10'into @FECHA, @REFERENCIA, @CARGO, @' +
              'ABONO, @SALDO'#10#10'WHILE @@FETCH_STATUS = 0'#10'BEGIN'#10'   SET @SALDOTOTAL' +
              ' =  @SALDOTOTAL + (@CARGO - @ABONO)'#10#10'   INSERT INTO #TEMP(FECHA,' +
              'REFERENCIA,CARGO,ABONO,SALDO)VALUES(@FECHA,@REFERENCIA,@CARGO,@A' +
              'BONO,@SALDOTOTAL)'#10'   FETCH NEXT FROM prueba_Cursor'#10'   into @FECH' +
              'A, @REFERENCIA, @CARGO, @ABONO, @SALDO'#10#10'END'#10#10'SELECT * FROM #TEMP' +
              #10'order by FECHA ASC'#10#10'DROP TABLE #TEMP'#10'CLOSE prueba_Cursor'#10'DEALLO' +
              'CATE prueba_Cursor'#10
            StatementType = stSQL
            ColumnMappings = <
              item
                DatasetField = 'FECHA'
                TableField = 'FECHA'
              end
              item
                DatasetField = 'REFERENCIA'
                TableField = 'REFERENCIA'
              end
              item
                DatasetField = 'CARGO'
                TableField = 'CARGO'
              end
              item
                DatasetField = 'ABONO'
                TableField = 'ABONO'
              end
              item
                DatasetField = 'SALDO'
                TableField = 'SALDO'
              end>
          end>
        Name = 'spSaldosDespachador'
        Fields = <
          item
            Name = 'FECHA'
            DataType = datDateTime
          end
          item
            Name = 'REFERENCIA'
            DataType = datString
            Size = 100
          end
          item
            Name = 'CARGO'
            DataType = datInteger
          end
          item
            Name = 'ABONO'
            DataType = datInteger
          end
          item
            Name = 'SALDO'
            DataType = datInteger
          end>
      end
      item
        Params = <
          item
            Name = 'PUNTOS'
            Value = '9'
            ParamType = daptInput
          end>
        Statements = <
          item
            Connection = 'Administracion'
            Default = True
            Name = 'Administracion'
            SQL = 
              'DECLARE @PUNTOS INT'#10'SET @PUNTOS = (SELECT :PUNTOS)'#10#10'SELECT * FRO' +
              'M ARTICULOS WHERE PUNTOS <= @PUNTOS'#10
